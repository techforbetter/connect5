{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true

  },
  "extends": "airbnb",
  "rules": {
    "indent": ["error", 2],
    "react/prop-types": [0],
    "react/no-did-update-set-state": [0, "disallow-in-func"],
    "no-underscore-dangle": [0],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prefer-stateless-function": [
      0,
      {
        "ignorePureComponents": true
      }
    ],
    "quotes": ["error", "double"],
    "react/forbid-prop-types": [0]
  }
}
